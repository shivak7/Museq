instrument Clean {
    waveform sine
    envelope 0.1 0.1 0.8 0.1
}

instrument Dirty {
    waveform sine
    envelope 0.1 0.1 0.8 0.1
    effect distortion 10.0
    effect bitcrush 4.0
}

instrument Echo {
    waveform sawtooth
    envelope 0.05 0.2 0.5 0.5
    effect delay 300 0.6
}

sequential {
    Clean { notes C4(500) }
    Dirty { notes C4(500) }
    Echo  { notes C4(500) }
}