instrument Kick {
    waveform sine
    envelope 0.01 0.1 0.2 0.1
}

instrument Violin {
    waveform sawtooth
    envelope 0.2 0.2 0.7 0.3
    portamento 200
}

sequential {
    parallel {
        repeat 4 {
            Kick { notes C2 }
        }
        
        Violin {
            tempo 60
            notes C4, G4, F4, E4
        }
    }
}
