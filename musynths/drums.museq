instrument SynthKick {
    waveform sine
    envelope 0.01 0.1 0.0 0.05
    gain 1.2
}

instrument SynthSnare {
    waveform square
    envelope 0.01 0.05 0.0 0.05
    filter highpass 1000 1.0
}

instrument SynthHat {
    waveform sawtooth
    envelope 0.005 0.03 0.0 0.01
    filter highpass 5000 0.5
}
