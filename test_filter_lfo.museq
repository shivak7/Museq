instrument WahBass {
    waveform sawtooth
    envelope 0.01 0.1 0.8 0.1
    filter lowpass 1000 4.0
    lfo cutoff sine 4 800
    sequence {
        note C2 2000 127
    }
}

instrument TremoloPad {
    waveform triangle
    envelope 0.5 0.5 0.8 0.5
    lfo amplitude sine 6 0.5
    sequence {
        note E4 2000 80
    }
}

instrument VibratoLead {
    waveform square
    envelope 0.1 0.1 0.8 0.1
    lfo pitch sine 5 0.5
    sequence {
        note G4 2000 100
    }
}

parallel {
    WahBass {}
    TremoloPad {}
    VibratoLead {}
}
